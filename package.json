{
  "name": "mike-hooks",
  "version": "1.0.9",
  "description": "",
  "prettier": {
    "printWidth": 80,
    "singleQuote": false,
    "semi": true
  },
  "main": "src/index.js",
  "bin": "lib/index.js",
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "plugins": [
      "prodperfect-testcafe"
    ],
    "rules": {
      "prodperfect-testcafe/no-debug-statements": "error",
      "prodperfect-testcafe/no-only-statements": "error"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "package-lock.json",
    "package.json"
  ],
  "scripts": {
    "build": "babel src --out-dir lib",
    "commit": "node lib/index.js"
  },
  "files": [
    "LICENSE",
    "README.md",
    "bin",
    "lib"
  ],
  "author": "",
  "license": "ISC",
  "np": {
    "contents": "lib"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/runtime-corejs3": "^7.8.4"
  },
  "dependencies": {
    "@arkweid/lefthook": "0.7.1",
    "chalk": "^3.0.0",
    "core-js": "^2.6.11",
    "eslint": "^6.8.0",
    "eslint-plugin-prodperfect-testcafe": "^1.0.7",
    "prettier": "1.19.1",
    "prodperfect-testcafe": "^1.0.0"
  }
}
